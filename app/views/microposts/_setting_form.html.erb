<%= f.fields_for :settings do |i| %>
  <div class="row">
    <div class="col-md-9">
      <div class = 'form-group'>
        <%= i.text_field :name, class: 'form-control setting_name' %>
      </div>
    </div>
    <div class="col-md-3">
      <div class = 'form-group'>
        <%= i.text_field :value, class: 'form-control setting_value' %>
      </div>
    </div>
  </div>
<% end %>
